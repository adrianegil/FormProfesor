<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.proyecto.gilsoft.formprofesor">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".app.MainActivity" />
        <activity android:name=".app.InformacionActivity" android:parentActivityName=".app.MainActivity" />
        <activity android:name=".app.FormularioActivity" />
        <activity android:name=".app.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".app.PregSeleccionActivity" />
        <activity android:name=".app.PregNumActivity" />
        <activity android:name=".app.PregVerdFalsoActivity" />
        <activity android:name=".app.VisualizarPregSeleccActivity" />
        <activity android:name=".app.VisualizarPregNumActivity" />
        <activity android:name=".app.VisualizarPregVerdFalsoActivity" />
        <activity android:name=".app.AyudaActivity" android:parentActivityName=".app.MainActivity"></activity>


    </application>

</manifest>